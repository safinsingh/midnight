[
  {
    "os": "Ubuntu",
    "version": 16,
    "description": "Password encryption algorithm set to SHA512",
    "control": {
      "type": "file",
      "location": "/etc/login.defs",
      "key": "ENCRYPT_METHOD",
      "value": "SHA512"
    }
  },
  {
    "os": "Ubuntu",
    "version": 16,
    "description": "Maximum Password Age set to 90 Days",
    "control": {
      "type": "file",
      "location": "/etc/login.defs",
      "key": "PASS_MAX_DAYS",
      "value": "90"
    }
  }
]
